{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-no-circular-imports"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-namespace": false,
    "object-literal-sort-keys": false,
    "ordered-imports": true,
    "interface-name": false,
    "interface-over-type-literal": false,
    "no-console": false,
    "ban-types": false,
    "no-shadowed-variable": false,
    "no-bitwise": false
  }
}
