{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "sourceMap": true,
    "target": "es2015",
    "lib": [
      "dom",
      "es2015"
    ],
    "baseUrl": "../../",
    "paths": {
      "@typed/*": [
        "packages/*",
        "node_modules/@typed/*"
      ]
    }
  },
  "files": [
    "./source/app.ts"
  ]
}
