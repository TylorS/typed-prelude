{
  "name": "Typed",
  "mode": "modules",
  "out": "docs",
  "theme": "pages-plugin",
  "pages": "./documentation/pages.json",
  "readme": "./documentation/readme.md",
  "ignoreCompilerErrors": "true",
  "includeVersion": "false",
  "exclude": "**/*+(test|browser-test).ts",
  "external-modulemap": ".*packages/([^/]+)/source/.*",
  "stripInternal": "true"
}
