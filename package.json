{
  "name": "@typed/prelude",
  "version": "5.0.0",
  "description": "The TypeScript Standard Library",
  "repository": "https://github.com/TylorS/typed-prelude",
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "main:ts": "./source/index.ts",
  "types": "./esm/index.d.ts",
  "scripts": {
    "build:cjs": "rm -rf cjs && ttsc --module commonjs --outDir cjs",
    "build:esm": "rm -rf esm && ttsc --module es2015 --outDir esm",
    "build": "npm run build:cjs && npm run build:esm",
    "test:build": "ttsc --module es2015 --outDir .tmp && rm -rf .tmp",
    "lint": "tslint -p tsconfig.json --fix",
    "test": "typed-test && npm run lint && npm run test:build",
    "preversion": "npm test && npm run build",
    "postversion": "npm publish"
  },
  "devDependencies": {
    "@typed/test": "9.2.0",
    "@types/node": "11.13.6",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "ts-node": "8.1.0",
    "tslib": "^1.9.3",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-no-circular-imports": "^0.6.2",
    "tslint-plugin-prettier": "2.0.1",
    "ttypescript": "^1.5.6",
    "typescript": "^3.4.4",
    "typescript-transform-paths": "^1.0.1"
  },
  "peerDependencies": {
    "typescript": "^3.4.4",
    "tslib": "^1.9.3"
  }
}
