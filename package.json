{
  "name": "@typed/prelude",
  "version": "5.0.0",
  "description": "The TypeScript Standard Library",
  "main": "lib/index.js",
  "module": "./index.js",
  "typings": "index.d.ts",
  "types": "index.d.ts",
  "repository": "https://github.com/TylorS/typed-test",
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean:commonjs": "rm -rf lib",
    "clean:es2015": "ts-node tools/cleanup.ts",
    "clean:all": "npm run clean:commonjs && npm run clean:es2015",
    "build:commonjs": "npm run clean:commonjs && tsc --module commonjs --outDir lib",
    "build:es2015": "npm run clean:es2015 && tsc --module es2015 --outDir ./",
    "build": "npm run build:commonjs && npm run build:es2015",
    "test": "typed-test",
    "preversion": "npm test && npm run build",
    "postversion": "npm publish",
    "postpublish": "npm run clean:all"
  },
  "devDependencies": {
    "@typed/test": "9.0.0",
    "@types/node": "10.12.18",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "^3.4.0-dev.20190326"
  },
  "peerDependencies": {
    "typescript": "^3.4.0-dev.20190326"
  },
  "dependencies": {
    "@most/disposable": "1.2.1",
    "@most/scheduler": "1.2.2",
    "@most/types": "1.0.1"
  }
}
