    "test": "typed-test",
    "preversion": "npm test && npm run build",
    "postversion": "npm publish"
  },
  "devDependencies": {
    "@typed/test": "7.2.2",
    "@types/node": "10.11.3",
    "@types/react": "16.4.14",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-plugin-prettier": "2.0.0",
    "typescript": "3.1.1"
  },
  "dependencies": {
    "@most/core": "1.3.2",
    "@most/disposable": "1.2.1",
    "@most/scheduler": "1.2.1",
    "@most/types": "1.0.1"
  }
}
