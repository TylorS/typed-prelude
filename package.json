{
  "name": "@typed/prelude",
  "version": "5.0.0",
  "description": "The TypeScript Standard Library",
  "main": "lib/index.js",
  "module": "./index.js",
  "typings": "index.d.ts",
  "types": "index.d.ts",
  "repository": "https://github.com/TylorS/typed-test",
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean:commonjs": "rm -rf lib",
    "clean:es2015": "ts-node tools/cleanup.ts",
    "clean:all": "npm run clean:commonjs && npm run clean:es2015",
    "build:commonjs": "npm run clean:commonjs && tsc --module commonjs --outDir lib",
    "build:es2015": "npm run clean:es2015 && tsc --module es2015 --outDir ./",
    "build": "npm run build:commonjs && npm run build:es2015",
    "test": "typed-test",
    "preversion": "npm test && npm run build",
    "postversion": "npm publish",
    "postpublish": "npm run clean:all"
  },
  "devDependencies": {
    "@typed/test": "9.0.0",
    "@types/minimatch": "^3.0.3",
    "@types/node": "10.12.18",
    "@types/power-assert": "^1.5.0",
    "@types/resolve": "0.0.8",
    "@types/rimraf": "^2.0.2",
    "@types/source-map-support": "^0.4.1",
    "@types/tempy": "^0.2.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "ts-node": "7.0.1",
    "tslint": "5.12.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.2.2"
  },
  "peerDependencies": {
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@most/disposable": "1.2.1",
    "@most/scheduler": "1.2.1",
    "@most/types": "1.0.1",
    "espower-source": "^2.3.0",
    "fuse-box": "^3.6.0",
    "minimatch": "^3.0.4",
    "power-assert": "^1.6.1",
    "resolve": "^1.9.0",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.9",
    "tempy": "^0.2.1",
    "tsconfig-paths": "^3.7.0"
  }
}
